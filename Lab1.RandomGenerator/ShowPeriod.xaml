<Window x:Class="Lab1.RandomGenerator.ShowPeriodWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1.RandomGenerator"
        mc:Ignorable="d"
        Title="ShowPeriod" Height="300" Width="250">
    <Grid>
        <TextBlock x:Name="PeriodTextBlock" Margin="10,41,10,160" TextWrapping="Wrap"/>
        <Label Content="Period:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="PeriodCountTextBlock" HorizontalAlignment="Left" VerticalAlignment="Top" Width="94" Margin="69,15,0,0"/>
        <TextBlock x:Name="APeriodTextBlock" Margin="10,160,10,30" TextWrapping="Wrap"/>
        <Label Content="APeriod:" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center"/>
        <TextBlock x:Name="APeriodCountTextBlock" HorizontalAlignment="Left" VerticalAlignment="Center" Width="94" Margin="69,0,0,0"/>
    </Grid>
</Window>
